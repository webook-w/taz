<script>
    // تحميل بيانات الفعاليات من LocalStorage عند تحميل الصفحة
    function loadEventData() {
        for (let i = 1; i <= 6; i++) {
            var eventData = JSON.parse(localStorage.getItem('event' + i));
            if (eventData) {
                document.getElementById('eventTitleText' + i).textContent = eventData.title;
                document.querySelector('#eventSection' + i + ' .day').textContent = eventData.date.split(' ')[0];
                document.querySelector('#eventSection' + i + ' .month').textContent = eventData.date.split(' ')[1];
                document.querySelector('#eventSection' + i + ' img').src = eventData.image;
                document.querySelector('#eventSection' + i + ' .address').textContent = eventData.location;
                document.querySelector('#eventSection' + i + ' .time').textContent = eventData.time;
            }
        }
    }

    window.addEventListener('load', loadEventData);
</script>
