        $(document).on("click", "#load_more_button", function () {
            //var start = $(this).data('start');
            $("#load_more_button").html(
                "<img src='https://golden4tic.com/front/assets/images/loader.svg' width='50' /> جارى التحميل.."
            );
            $.ajax({
                url: "https://golden4tic.com?start=" + $("#start").val(),
                method: "GET",

                success: function (data) {
                    $("#start").remove();

                    var $item = $(data);
                    $("#load_more_button").html("عرض مزيد من الفعاليات");
                    $(".events-content").append($item);
                    if ($("#start").val() >= "4") {
                        $("#load_more_button").hide();
                    }
                },
            });
        });

        function filter() {
            $(".events-content").html(
                "<img src='https://golden4tic.com/front/assets/images/loader.svg' width='50' /> جارى التحميل.."
            );
            $("#clrea-filter").html(
                "<a href='https://golden4tic.com'>إلغاء التصفية</a>"
            );
            $(".events-content").load(
                "https://golden4tic.com/filter",
                $("#filterForm").serialize(),
                function (response, status, xhr) {
                    if ($("#noEvents")) {
                        $("#load_more_button").remove();
                    } else {
                        $("#load_more_button").show();
                    }
                }
            );
        }

      // $(document).on('click', '#search_events', function(){
